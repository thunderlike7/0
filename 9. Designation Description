#!/bin/sh
echo enter the code
read code
echo enter the des
read des
if echo $code | egrep -q^[0-9]+$"; then
echo $code is a number
else
fi
echo $code is not a number
if echo $des | egrep -q ^[a-zA-Z]+$"; then
echo $des has all alphabets
else
echo $des does not have all alphabets
fi
echo $code Sdes >> f1.txt
